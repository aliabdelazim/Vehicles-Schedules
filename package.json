{
  "name": "reason-labs",
  "version": "0.0.1",
  "description": "NestJS 8",
  "author": "Ali Abdelazim",
  "homepage": "https://lucha.at",
  "license": "MIT",
  "contributors": [
    "Ali Isaac"
  ],
  "keywords": [
    "nest",
    "nest js",
    "nestjs",
    "nest-js",
    "eslint"
  ],
  "private": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "ng": "ng",
    "build-nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start-nest": "nest start",
    "start-nest:dev": "nest start --watch",
    "start-nest:debug": "nest start --debug --watch",
    "start-nest:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^7.1.3",
    "class-validator": "^0.14.0",
    "classlist.js": "1.1.20150312",
    "intl": "1.2.5",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.6.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.25",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^29.3.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.5.4"
  },
  "jest": {
    "moduleVehicleExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
